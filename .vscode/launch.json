    "version": "0.2.0",
    "configurations": [
        {
            "name": "Geo-MacOs",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/src/Geo/cmd/tinder-geo",
            "env": {
                "CONFIG_PATH":"../../config/config.yaml",
                "CGO_ENABLED":"1"
            },
        },
        {
            "name": "Reaction-MacOs",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/src/Reaction/cmd/tinder-reaction",
            "env": {
                "CONFIG_PATH":"../../config/config.yaml",
                "CGO_ENABLED":"1"
            },
        },
        {
            "name": "Geo-Windows",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/src/Geo/cmd/tinder-geo",
            "env": {
                "CONFIG_PATH":"../../config/config.yaml",
                "CGO_ENABLED":"1",
                "CC":"C:\\TDM-GCC-64\\bin\\gcc"
            },
        }
    ]
}